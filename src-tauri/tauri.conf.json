{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "zap-rs",
  "version": "0.1.0",
  "identifier": "com.zap-rs.app",
  "build": {
    "frontendDist": null
  },
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "dragDropEnabled": true,
        "label":"main",
        "title": "zap-rs",
